#ifndef INC_ARDUCAM_H_
#define INC_ARDUCAM_H_

typedef struct arducam_info_t {
	SPI_HandleTypeDef *spi;
	I2C_HandleTypeDef *i2c;
	res_t resolution;
} arducam_info_t;

typedef enum res_t {
	_1800x16000,
	_800x600,
} res_t;

#endif /* INC_ARDUCAM_H_ */
